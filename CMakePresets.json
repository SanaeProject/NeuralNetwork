{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21
  },
  "configurePresets": [
    {
      "name": "x64-debug-with-openblas-vs",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "USE_OPENBLAS": "ON",
        "CMAKE_TOOLCHAIN_FILE": "C:/vcpkg/scripts/buildsystems/vcpkg.cmake"
      }
    },
    {
      "name": "x64-debug-without-openblas-vs",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "USE_OPENBLAS": "OFF"
      }
    },
    {
      "name": "x64-release-with-openblas-vs",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "USE_OPENBLAS": "ON",
        "CMAKE_TOOLCHAIN_FILE": "C:/vcpkg/scripts/buildsystems/vcpkg.cmake",
        "CMAKE_CXX_FLAGS": "/O2 /arch:AVX2"
      }
    },
    {
      "name": "x64-release-without-openblas-vs",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "USE_OPENBLAS": "OFF",
        "CMAKE_CXX_FLAGS": "/O2 /arch:AVX2"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "x64-debug-with-openblas-vs",
      "configurePreset": "x64-debug-with-openblas-vs"
    },
    {
      "name": "x64-debug-without-openblas-vs",
      "configurePreset": "x64-debug-without-openblas-vs"
    },
    {
      "name": "x64-release-with-openblas-vs",
      "configurePreset": "x64-release-with-openblas-vs"
    },
    {
      "name": "x64-release-without-openblas-vs",
      "configurePreset": "x64-release-without-openblas-vs"
    }
  ]
}